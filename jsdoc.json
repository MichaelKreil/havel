{	
	"source": {
		"include": ["Readme.md", "lib/"],
		"includePattern": ".+\\.js$"
  },
	"opts": {
		"template": "./node_modules/jaguarjs-jsdoc/",
		"encoding": "utf8",
		"destination": "./docs/"
  },
	"templates": {
		"applicationName": "Havel",
		"plugins": ["plugins/markdown"],
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
		"dateFormat": "YYYY-MM-DD, hh:mm:ss"
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
  }
}